Cell[TextData[{
 "\n\n",
 StyleBox["Esempio:\nConsidero una funzione polidroma integranda per una \
funzione ipergeometrica  F[\[Alpha],\[Gamma]; z] a coefficienti \[Alpha] ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    FractionBox["3", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm",
